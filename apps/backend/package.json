{
  "name": "@repo/backend",
  "private": true,
  "version": "0.0.0",
  "packageManager": "bun@1.2.22",
  "scripts": {
    "dev": "bun run --watch src/server.ts",
    "build": "tsc --noEmit && bun build --target bun --minify-whitespace --minify-syntax --outfile ./dist/index.js src/server.ts",
    "start": "bun ./dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "scripts:create-admin": "bun run scripts/create-admin.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.895.0",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/cron": "^1.4.1",
    "@elysiajs/openapi": "^1.4.10",
    "@prisma/client": "^6.16.2",
    "@t3-oss/env-core": "^0.13.8",
    "better-auth": "^1.3.16",
    "elysia": "^1.4.7",
    "elysia-helmet": "^3.0.0",
    "elysia-rate-limit": "^4.4.0",
    "logixlysia": "^5.1.1",
    "prismabox": "^1.1.25",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@inquirer/prompts": "^7.8.6",
    "@repo/tsconfig": "workspace:*",
    "@types/bun": "^1.2.22",
    "prisma": "^6.16.2",
    "typescript": "^5.9.2"
  }
}
